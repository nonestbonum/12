#include "pch.h"
#include <iostream>
using namespace std;
int main()
{
	double kilometer, liter;
	const double sto = 100;
	cout << "Enter the number of kilometers: ";
	cin >> kilometer;
	cout << "Enter the number of liters: ";
	cin >> liter;
	cout << "Your fuel consumption: " << liter / (kilometer / sto);
	return 0;
}
