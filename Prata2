#include "stafx.h"
#include "pch.h"
#include <iostream>
using namespace std;
double temperature(double Celsius)
{
    return 1.8 * celsius + 32;
}
int main()
{
    double Fahrenheit;
    double Celsius;
    cout << "enter temperature in Celsius\n";
    cin >> Celsius;
    Fahrenheit = temperature(celsius);
    cout << "temperature in Fahrenheit: " << fahrenheit << endl;
    return 0;
}
